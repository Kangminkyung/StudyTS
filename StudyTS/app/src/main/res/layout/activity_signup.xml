<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/pw_wrong_text"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignupActivity">

    <EditText
        android:id="@+id/email_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="35dp" />

    <EditText
        android:id="@+id/pw_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="212dp" />

    <EditText
        android:id="@+id/pwagain_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="267dp" />

    <EditText
        android:id="@+id/nickname_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="nick name"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="364dp" />

    <Button
        android:id="@+id/check_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="check"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="33dp" />

    <Button
        android:id="@+id/register_botton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="register"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="432dp" />

    <TextView
        android:id="@+id/exist_error_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="The e-mail address alreay exist."
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="90dp" />

    <TextView
        android:id="@+id/pw_wrong_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="wrong password"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="320dp" />

    <EditText
        android:id="@+id/email_check_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="134dp" />

    <Button
        android:id="@+id/send_test_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="send"
        tools:layout_editor_absoluteX="279dp"
        tools:layout_editor_absoluteY="131dp" />

    <TextView
        android:id="@+id/email_enable_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="passed"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="192dp" />

</android.support.constraint.ConstraintLayout>